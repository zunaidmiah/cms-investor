{
    "name": "bllim/laravel-to-jquery-validation",
    "description": "This package makes validation rules defined in laravel work client-side by converting to jquery validation rules. It uses Jquery Validation Plugin. It also allows to use laravel validation messages so you can show same messages for both sides.",
    "keywords" : ["laravel4","laravel","jquery","validation","laravel validation","laravel validation converter","laravel to jquery validation", "laravel validation to jquery validation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bilal Gultekin",
            "email": "bilal@bilal.im"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/html": ">=4.2",
        "illuminate/support": ">=4.2",
        "illuminate/validation": ">=4.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Bllim\\LaravelToJqueryValidation\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
